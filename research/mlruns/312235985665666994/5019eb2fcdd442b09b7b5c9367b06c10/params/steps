[('transform', ColumnTransformer(transformers=[('num', StandardScaler(),
                                 ['age', 'trestbps', 'thalach', 'oldpeak']),
                                ('cat', TargetEncoder(),
                                 ['sex', 'cp', 'fbs', 'restecg', 'exang',
                                  'slope', 'ca', 'thal', 'age_group'])])), ('model', RandomForestClassifier(random_state=43))]